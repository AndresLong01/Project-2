<div class="container-fluid">
    <div class="main">
        {{> header splash=false}}
        <div class="row">
            {{#each art}}
            <div class="col-xl-4 col-m-6 col-xs-1">
                <img src="{{ artUrl }}" alt="{{ artTitle }}"></img>
                <p>{{ artTitle }}</p>
                <p>{{ artist }}</p>
            </div>
            {{/each}}
        </div>

    </div>
</div>